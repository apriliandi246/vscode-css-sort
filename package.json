{
   "name": "css-sort",
   "displayName": "css-sort",
   "description": "Sort your css properties and make it easy to read....",
   "version": "0.8.0",
   "publisher": "apriliandi246",
   "engines": {
      "vscode": "^1.46.0"
   },
   "galleryBanner": {
      "color": "#C80000",
      "theme": "dark"
   },
   "icon": "images/my-logo.jpg",
   "license": "MIT",
   "categories": [
      "Other"
   ],
   "activationEvents": [
      "onCommand:css-sort.min",
      "onCommand:css-sort.max"
   ],
   "main": "./extension.js",
   "contributes": {
      "commands": [
         {
            "command": "css-sort.max",
            "title": "Css Sort - max  ðŸ‘†"
         },
         {
            "command": "css-sort.min",
            "title": "Css Sort - min    ðŸ‘‡"
         }
      ],
      "menus": {
         "editor/context": [
            {
               "command": "css-sort.max",
               "group": "sort-it@1",
               "when": "resourceLangId == html && editorHasSelection"
            },
            {
               "command": "css-sort.min",
               "group": "sort-it@2",
               "when": "resourceLangId == html && editorHasSelection"
            },
            {
               "command": "css-sort.max",
               "group": "sort-it@3",
               "when": "resourceLangId == css && editorHasSelection"
            },
            {
               "command": "css-sort.min",
               "group": "sort-it@4",
               "when": "resourceLangId == css && editorHasSelection"
            },
            {
               "command": "css-sort.max",
               "group": "sort-it@5",
               "when": "resourceLangId == svelte && editorHasSelection"
            },
            {
               "command": "css-sort.min",
               "group": "sort-it@6",
               "when": "resourceLangId == svelte && editorHasSelection"
            },
            {
               "command": "css-sort.max",
               "group": "sort-it@7",
               "when": "resourceLangId == vue && editorHasSelection"
            },
            {
               "command": "css-sort.min",
               "group": "sort-it@8",
               "when": "resourceLangId == vue && editorHasSelection"
            }
         ]
      }
   },
   "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "node ./test/runTest.js",
      "vscode:prepublish": "webpack --mode production",
      "webpack": "webpack --mode development",
      "webpack-dev": "webpack --mode development --watch"
   },
   "devDependencies": {
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.0.0",
      "@types/node": "^12.11.7",
      "@types/vscode": "^1.46.0",
      "eslint": "^7.9.0",
      "glob": "^7.1.6",
      "mocha": "^8.1.3",
      "typescript": "^4.0.2",
      "vscode-test": "^1.4.0",
      "webpack": "^5.11.0",
      "webpack-cli": "^4.2.0"
   },
   "dependencies": {}
}
