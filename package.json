{
   "name": "css-sort",
   "displayName": "css-sort",
   "description": "Sort your css properties and make it easy to read....",
   "version": "0.9.0",
   "engines": {
      "vscode": "^1.52.0"
   },
   "publisher": "apriliandi246",
   "categories": [
      "Other"
   ],
   "icon": "images/my-logo.jpg",
   "activationEvents": [
      "onCommand:css-sort.min",
      "onCommand:css-sort.max"
   ],
   "main": "./dist/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "css-sort.min",
            "title": "Css Sort - min    ðŸ‘‡"
         },
         {
            "command": "css-sort.max",
            "title": "Css Sort - max   ðŸ‘†"
         }
      ],
      "menus": {
         "editor/context": [
            {
               "command": "css-sort.max",
               "group": "sort-it@1",
               "when": "resourceLangId == html && editorHasSelection"
            },
            {
               "command": "css-sort.min",
               "group": "sort-it@2",
               "when": "resourceLangId == html && editorHasSelection"
            },
            {
               "command": "css-sort.max",
               "group": "sort-it@3",
               "when": "resourceLangId == css && editorHasSelection"
            },
            {
               "command": "css-sort.min",
               "group": "sort-it@4",
               "when": "resourceLangId == css && editorHasSelection"
            },
            {
               "command": "css-sort.max",
               "group": "sort-it@5",
               "when": "resourceLangId == svelte && editorHasSelection"
            },
            {
               "command": "css-sort.min",
               "group": "sort-it@6",
               "when": "resourceLangId == svelte && editorHasSelection"
            },
            {
               "command": "css-sort.max",
               "group": "sort-it@7",
               "when": "resourceLangId == vue && editorHasSelection"
            },
            {
               "command": "css-sort.min",
               "group": "sort-it@8",
               "when": "resourceLangId == vue && editorHasSelection"
            }
         ]
      }
   },
   "scripts": {
      "vscode:prepublish": "npm run package",
      "compile": "webpack --config ./build/node-extension.webpack.config.js",
      "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
      "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
      "test-compile": "tsc -p ./",
      "test-watch": "tsc -watch -p ./",
      "pretest": "npm run test-compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
   },
   "devDependencies": {
      "@types/css-tree": "^1.0.4",
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.0.4",
      "@types/node": "^12.11.7",
      "@types/vscode": "^1.52.0",
      "@typescript-eslint/eslint-plugin": "^4.9.0",
      "@typescript-eslint/parser": "^4.9.0",
      "eslint": "^7.15.0",
      "glob": "^7.1.6",
      "mocha": "^8.1.3",
      "ts-loader": "^8.0.11",
      "typescript": "^4.1.2",
      "vscode-test": "^1.4.1",
      "webpack": "^5.10.0",
      "webpack-cli": "^4.2.0"
   },
   "dependencies": {
      "css-tree": "^1.1.2",
      "csstree-validator": "^2.0.0"
   }
}
